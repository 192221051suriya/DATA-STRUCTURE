#include <stdio.h>

struct Node {
    int data;
    struct Node* next;
};

int main() {

    struct Node n1, n2, n3, n4;


    n1.data = 6;
    n2.data = 7;
    n3.data = 8;
    n4.data = 9;

    /* Link nodes */
    n1.next = &n2;
    n2.next = &n3;
    n3.next = &n4;
    n4.next = NULL;

    /* Display linked list */
    struct Node* temp = &n1;
    while (temp != NULL) {
        printf("%d", temp->data);
        if (temp->next != NULL)
            printf("->");
        temp = temp->next;
    }

    return 0;
}
