#include<stdio.h>
#include<math.h>
int main(){
int a[i][j],b[i][j],sum[i][j],i,j;
printf("enter a matrix");
for(i=0;i<2;i++){
	for(j=0;j<2;j++)
		scanf("%d",&a[i][j]);
}
printf("enter b matrix");
	for(i=0;i<2;i++)
		scanf("%d",&b[i][j]);
printf("addition of two matrix");
for(i=0;i<2;i++){
	for(j=0;j<2;j++)
		sum[i][j]=a[i][j]+b[i][j];
}
printf("print result");
for(i=0;i<2;i++){
	for(j=0;j<2;j++)
		scanf("%d",sum[i][j]);
}
getch();
return 0;
}
