#include <stdio.h>

int main() {
    int n, i, j;
    int found;

    scanf("%d", &n);

    int arr[n];

    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    /* Check from 1 to n+1 */
    for (i = 1; i <= n + 1; i++) {
        found = 0;

        for (j = 0; j < n; j++) {
            if (arr[j] == i) {
                found = 1;
                break;
            }
        }

        if (!found) {
            printf("%d", i);
            break;
        }
    }

    return 0;
}
