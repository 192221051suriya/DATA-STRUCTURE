#include <stdio.h>

int main() {
    char s[] = "tree";   /* change string here if needed */
    char temp;
    int i, j, n = 0;

    /* Find length */
    while (s[n] != '\0')
        n++;

    /* Sort string */
    for (i = 0; i < n - 1; i++) {
        for (j = i + 1; j < n; j++) {
            if (s[i] > s[j]) {
                temp = s[i];
                s[i] = s[j];
                s[j] = temp;
            }
        }
    }

    /* Print sorted string */
    printf("%s\n", s);

    /* Print starting index of repeated characters */
    for (i = 0; i < n - 1; i++) {
        if (s[i] == s[i + 1]) {
            printf("%d ", i);
            while (i < n - 1 && s[i] == s[i + 1])
                i++;
        }
    }

    return 0;
}

